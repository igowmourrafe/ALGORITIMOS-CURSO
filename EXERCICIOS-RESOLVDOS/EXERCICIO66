algoritmo "semnome"
var
   cont , n , tab: inteiro
inicio
   escreval(" XXXXXXXXXXXXXXXXXXXXXXXXXXX")
   escreval("  TABUADA DE MULTIPLICAÇÃO ")
   escreval(" XXXXXXXXXXXXXXXXXXXXXXXXXXX")
   escreval("")
   escreva(" DIGÍTE UM NÚMERO: ")
   leia(n)
   escreval()
   para cont <- 1 ate 10 faca
      tab <- n * cont
      escreval("",n,"  X ",cont," =",tab)
   fimpara
   escreval("")
fimalgoritmo

