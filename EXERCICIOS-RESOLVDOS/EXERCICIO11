algoritmo "semnome"
var

   A , B , C , Delta: real
inicio
    escreva("Valor de A: ")
   leia(A)
   escreva("Valor de B: ")
   leia(B)
   escreva("Valor de C: ")
   leia(C)
    Delta <- (B ^ 2) - 4 * A * C
    escreva(Delta)
fimalgoritmo

