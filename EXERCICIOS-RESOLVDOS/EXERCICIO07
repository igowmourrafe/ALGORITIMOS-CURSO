 algoritmo "tercaparte"
 var
  N1 , dob, ter: real
 inicio
 escreva("Digite um número :")
 leia(N1)
  dob <- (N1 * 2)
  ter <- (N1 / 3)
  escreval("O dobro de ",N1," é ",dob)
  escreval("A terça parte de ",N1," é ", ter)
 fimalgoritmo

