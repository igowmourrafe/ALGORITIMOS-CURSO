algoritmo "semnome"
var
   contador , num : inteiro
inicio
   contador <- 1
   escreva(" DIGITE UM VALOR: ")
   leia(num)
   escreval("")
   escreva(" CONTAGEM: ")
   enquanto ( contador <= num ) faca
      escreva(contador)
      contador <- contador + 1
   fimenquanto
   escreval(" ACABOU!")
   escreval("")
fimalgoritmo

