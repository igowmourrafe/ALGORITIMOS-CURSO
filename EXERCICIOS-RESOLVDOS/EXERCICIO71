algoritmo "semnome"
var
   v : vetor[1..8] de inteiro
   c : inteiro
inicio
   escreval(" V E T O R E S ")
   escreval(" ----------------------------------------------")
   escreval("")
   para c <- 1 ate 8 faca
      v[c] <- 999
      escreva(" ",v[c])
   fimpara
   escreval("")
fimalgoritmo

//71) Faça um programa que preencha automaticamente um vetor numérico com 8
//posições, conforme abaixo:
//999 999 999 999 999 999 999 999
// 0   1   2   3   4   5   6   7
