algoritmo "semnome"
var
   vet : vetor[0..9] de inteiro
   c , aux : inteiro
inicio
   escreval(" V|E|T|O|R|E|S")
   escreval("")
   para c <- 0 ate 9 faca
      aux <- aux + 5
      vet[c] <- aux
      escreva(vet[c])
   fimpara
   escreval("")
fimalgoritmo

