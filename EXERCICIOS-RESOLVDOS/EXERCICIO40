algoritmo "semnome"
var
 contador : inteiro
inicio
  contador <- 0
  enquanto ( contador <= 18 ) faca
   escreva("")
   escreva(contador)
   contador <- contador + 3
  fimenquanto
  escreva(" ACABOU!")
