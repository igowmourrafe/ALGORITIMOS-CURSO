algoritmo "semnome"
var
   vali , valf , incr : inteiro
inicio
   escreva(" Digite o primeiro Valor: ")
   leia(vali)
   escreva(" Digite o último Valor: ")
   leia(valf)
   escreva(" Digite o incremento: ")
   leia(incr)
   escreval("")
   enquanto (vali <= valf ) faca
      escreva(vali)
      vali <- vali + incr
   fimenquanto
   escreval(" ACABOU!")
fimalgoritmo

