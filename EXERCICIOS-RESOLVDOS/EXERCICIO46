algoritmo "semnome"
var
   contador , soma:inteiro
inicio
   contador <- 0
   soma <- 0
   escreva(" 0")
   enquanto ( contador <= 100 ) faca
      escreva(" +",contador)
      soma <- soma + contador
      contador <- contador + 2
   fimenquanto
   escreval(" = ",soma)
fimalgoritmo

