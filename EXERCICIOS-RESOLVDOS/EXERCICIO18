algoritmo "semnome"
var
    nome : caractere
    n1 , n2 , med : real
inicio
   escreva(" DIGITE SEU NOME: ")
   leia(nome)
   escreva(" PRIMEIRA NOTA: ")
   leia(n1)
   escreva(" SEGUNDA NOTA: ")
   leia(n2)
     med <- ( n1 + n2 ) / 2
   se ( med >= 7 ) entao
   escreval("ALUNO : ",nome,)
   escreval("---APROVADO---")
   escreval("A MÉDIA FOI ",med,)
fimalgoritmo

