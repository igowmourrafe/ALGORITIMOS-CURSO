algoritmo "semnome"
var
   preco, Des: real
inicio
   escreva("Preço do produto : ")
   leia(preco)
    Des <- (preco * 5) / 100
    Des <- preco - Des
   escreva("PREÇO PROMOCIONAL COM 5% DE DESCONTO:",Des," REAIS. ")
fimalgoritmo

